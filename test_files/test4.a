.data

msgPar: .asciiz "El numero es par"
msgImpar: .asciiz "El numero es impar"


.text

li $v0 5
syscall
move $t0 $v0  #$t0 n√∫mero introducido
li $t1 2

rem $t3 $t0 $t1
beqz $t3 par
impar:
	li $v0 4
	la $a0 msgImpar
	syscall
	jr $ra

par: 
	li $v0 4
	la $a0 msgPar
	syscall
	jr $ra








