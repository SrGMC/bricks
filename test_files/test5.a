
.data

msg: .ascii "Introduce un n√∫mero"

.text

li $v0 4
la $a0 msg
syscall

li $v0 5
syscall
move $t0 $v0  #$t0 = N

li $t1 0  # acumulador de la suma
li $t2 2  # int i = 2 (for)

for:
	bge $t2 $t0 fin
	add $t1 $t1 $t2  # donde vamos acumulando la suma de pares
	addi $t2 $t2 2  #i +2
	b for

fin:
	li $v0 1
	move $a0 $t1
	syscall
